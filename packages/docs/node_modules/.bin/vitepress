#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chenrongqi/毕设/项目/vue3-front-components/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chenrongqi/毕设/项目/vue3-front-components/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vitepress@1.0.0-alpha.75_sass@1.62.0/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vitepress@1.0.0-alpha.75_sass@1.62.0/node_modules/vitepress/bin/vitepress.js" "$@"
fi
